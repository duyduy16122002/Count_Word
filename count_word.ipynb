   "source": [
    "def count_word(text,word):\n",
    "    lst = text.split(' ')\n",
    "    count = 0\n",
    "    for i in range (len(lst)):\n",
    "        count += lst[i].count(word)\n",
    "    return count\n",
    "text = input('Nhập văn bản: ')\n",
    "word = input('Nhập từ cần đếm: ')\n",
    "print(count_word(text,word))"
   ]
